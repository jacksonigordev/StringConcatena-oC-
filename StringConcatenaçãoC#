namespace LÃ³gicaDeProgramacao02Aritimedicos
{
    internal class Program
    {
        static void Main(string[] args)
        {
            string primeiroNome, segundoNome, ultimoNome;

            primeiroNome = "Jackson ";
            segundoNome = "Igor ";
            ultimoNome = "Bastos";

